class staff:
 
 university_name = "Golden"
  
 def __init__ (self, name, age, salary, faculty, experiment, position, hire_date):
    self.name = name
    self.age = age
    self.salary = salary
    self.faculty = faculty
    self.experiment = experiment
    self.position = position
    sel.hire_date = hire_date



 def get_name (self):
     return self.name
    
 def get.age (self):
     return self.age

 def get_salary (self):
     return self.salary

 def get_faculty (self):
     return self.faculty

 def get_experiment (self):
     return self.experiment
      
 def get.position (self):
